public class Main {
    public static void main(String[] args) {
        int num = 2; 
        for (int i =( 2); i <= 4; i++) { 
            for (int j = 1; j <= i; j++) { 
                System.out.print((num+num) +" ");
                num++;
            }
            System.out.println(); 
        }
    }
}
