public class Nested {
    public static void main(String[] args) {
        for (int i = 1; i <= 3; i++) {
            System.out.println("outerloop:" + i);
            
            if (i == 1) {
                for (int j = 1; j <= 3; j++) {
                    System.out.println("innerloop:" + j);
                }
            }
        }
    }
}
