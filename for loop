public class BreakAndContinueExample {
    public static void main(String[] args) {
        for (int i = 1; i <= 10; i++) {

            if (i == 5) {
                System.out.println("Loop stopped at i = " + i);
                break; 
            }

            if (i == 10) {
                continue; 
            }

            System.out.println("i = " + i);
        }
    }
}
