public class CollegeData {
    public static void main(String[] args) {

        String studName = "Samitha Sri"; 
        String collegeName = "Mount Zion College of Engineering and Technology";
        byte currentYear = 3;                 
        short sprNo = 9165;                   
        int joiningYear = 2025;               
        long registrationNumber = 911723243022L; 
        float height = 150.0f;               
        char section = 'A';                   
        boolean isFemale = true;              
        
        System.out.println("---- Student College Profile ----");
        System.out.println("Student Name         : " + studName);
        System.out.println("College Name         : " + collegeName);
        System.out.println("Current Year         : " + currentYear);
        System.out.println("SPR Number           : " + sprNo);
        System.out.println("Year of Joining      : " + joiningYear);
        System.out.println("Registration Number  : " + registrationNumber);
        System.out.println("Height               : " + height + " cm");
        System.out.println("Section              : " + section);
        System.out.println("Gender (Female)      : " + isFemale);
    }
}
