public class Nested {
    public static void main(String[] args) {
        for (int i = 1; i <= 1; i++) { 
            System.out.println("outerloop:" + i);
            
        if (i == 1) {
        for (int j = 1; j <= 1; j++) { 
            System.out.println("middleloop:" + j);
                    
        for (int k = 1; k <= 1; k++) { 
            System.out.println("innerloop:" + k);
                    }
                }
            }
        }
    }
}
