public class Main {
    public static void main(String[] args) {
        int[][] arr = new int[3][3];  

        for (int i = 0; i < 3; i++) {
            arr[i][0] = 10;
            arr[i][1] = 20;
            arr[i][2] = 30;
        }

        for (int i = 0; i < 3; i++) {
            for (int j = 0; j < 3; j++) {
                System.out.print(arr[i][j] + " ");
            }
            System.out.println(); 
        }
    }
}
